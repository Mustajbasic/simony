{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "browser": true,
        "node": true
    },
    "extends": ["eslint:recommended", "google"],
    "rules": {
        "camelcase": "off",
        "new-cap": "off",
        "linebreak-style": "off",
        "no-console": "warn",
        "max-len": "off"
    }
  }